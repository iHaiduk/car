<div id="myCars" tabindex="-1" aria-hidden="false" class="modal fade in">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">×</button>
                <h4 id="myModalLabel" class="modal-title">Информация об автомобиле(-ях)</h4>
            </div>
            <% for (var num in user_cars) { %>
            <div class="panel" id="car_view_<%= user_cars[num].id %>">
                <div class="panel-body">
                    <div href="#" class="media p mt0">
                        <div class="media-body">
                            <div class="media-heading">
                                <h3 class="mt0"><%= user_cars[num].model_trim %></h3>
                                <div class="row row-table">
                                    <div class="col-xs-4">
                                        <ul class="list-unstyled">
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].model_transmission_type_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= __(user_cars[num].model_transmission_type) %></li>
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].model_body_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= user_cars[num].model_body %></li>
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].country_check == 0 && user_cars[num].region_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= user_cars[num].country %> (<em title="Предназначение региона"><%= user_cars[num].region %></em>)</li>
                                        </ul>
                                    </div>
                                    <div class="col-xs-4">
                                        <ul class="list-unstyled">
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].model_engine_cc_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= user_cars[num].model_engine_cc %> мЛ</li>
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].model_engine_ci_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= user_cars[num].model_engine_ci %> л.с.</li>
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].model_engine_torque_rpm_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= user_cars[num].model_engine_torque_rpm %> RPM</li>
                                        </ul>
                                    </div>
                                    <div class="col-xs-4">
                                        <ul class="list-unstyled">
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].model_length_mm_check == 0 &&user_cars[num].model_width_mm_check == 0 &&user_cars[num].model_height_mm_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= user_cars[num].model_length_mm %>х<%= user_cars[num].model_width_mm %>х<%= user_cars[num].model_height_mm %> мм</li>
                                            <li class="mb-sm">
                                                <em class="fa <% if(user_cars[num].model_weight_kg_check == 0){ %><%- "fa-circle-o" %> <% }else{ %> <%- "fa-circle" %> <% } %> fa-fw"></em><%= user_cars[num].model_weight_kg %> кг</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-body bg-inverse">
                    <div class="row row-table text-center">
                        <div class="col-xs-4">
                            <p class="m0 h3"><%= user_cars[num].vinCode %></p>
                            <p class="m0 text-muted">VIN</p>
                        </div>
                        <div class="col-xs-4">
                            <p class="m0 h3"><%= user_cars[num].make_name +" "+user_cars[num].model_name %></p>
                            <p class="m0 text-muted">Авто</p>
                        </div>
                        <div class="col-xs-4">
                            <p class="m0 h3"><%= user_cars[num].model_year %></p>
                            <p class="m0 text-muted">Год</p>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
    </div>
</div>